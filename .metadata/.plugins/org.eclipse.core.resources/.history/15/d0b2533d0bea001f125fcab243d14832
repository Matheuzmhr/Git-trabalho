package fundamentos.operadores.copy;

public class Unarios {
	public static void main(String[] args) {
		
		@SuppressWarnings("unused")
		int a = 1;
		@SuppressWarnings("unused")
		int b = 2;
		
		a++; // a = a + 1
		a--; // a = a - 1
		
		++b; // b = b + 1
		--b; // b = b - 1
	}

}
